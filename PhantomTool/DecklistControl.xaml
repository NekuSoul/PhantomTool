<UserControl x:Class="NekuSoul.PhantomTool.DecklistControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NekuSoul.PhantomTool"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="300">
    <DockPanel>
        <Label DockPanel.Dock="Top" Name="InfoLabel" Height="20"/>
        <Button DockPanel.Dock="Bottom" Click="ClipBoardButtonClicked">Copy to clipboard</Button>
        <ListBox Name="OutputListBoxA"/>
    </DockPanel>
</UserControl>
